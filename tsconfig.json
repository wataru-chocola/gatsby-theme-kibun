{
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./src/templates/page.module.scss.d.ts"
  ],
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react",
    "strict": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  }
}
